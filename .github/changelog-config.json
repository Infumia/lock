    {
      "title": "🛠 Maintenance",
      "labels": [
        "maintenance"
      ],
      "priority": 4
    },
    {
      "title": "📚 Documentation",
      "labels": [
        "documentation"
      ],
      "priority": 5
    },
    {
      "title": "🔧 Other Updates",
      "labels": [],
      "priority": 6
    }
  ],
  "ignore_labels": [
    "skip-changelog"
  ],
  "group_by": "labels",
  "sort": "priority",
  "change_template": "- {{subject}}{{#if author}} (by [{{author}}](https://github.com/{{author}})){{/if}}{{#if issues}} (closes {{#each issues}}[#{{this.number}}]({{this.url}}){{#unless @last}}, {{/unless}}{{/each}}){{/if}} [#[{{pullRequestNumber}}]({{pullRequestUrl}})]",
  "highlights": {
    "enabled": true,
    "labels": [
      "breaking",
      "enhancement"
    ],
    "title": "🚀 Highlights"
  }
